{
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.ts(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "recurse": true,
      "destination": "./docs"
    }
  }